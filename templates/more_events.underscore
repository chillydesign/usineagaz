<table id="more_events">
    <tbody>
  <%  _.each(events,function(event,key,list){  %>


      <tr>
        <td class="event_date"><%= event.the_day %> <%= event.the_month_text %></td>
        <td class="event_title"><a href="<%= event.the_usine_link %>"><%= event.the_title %></a></td>
        <td class="event_info"><% if (event.the_category) { %> <%=  event.the_style %><% } %></td>
      </tr>

      <% }) %>
    </tbody>
  </table>
